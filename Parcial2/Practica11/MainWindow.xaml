    <Grid Margin="0,0,0,-16">
        <Grid.Background>
            <ImageBrush ImageSource="/655991.jpg"/>
        </Grid.Background>
        <Rectangle x:Name="rectangle" Height="100" Stroke="Black" Width="100" Margin="124,97,576,237" RenderTransformOrigin="0.5,0.5">
            <Rectangle.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Rectangle.RenderTransform>
            <Rectangle.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FF24256C"/>
                    <GradientStop Color="#FF7C7C7C" Offset="1"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
        <Button x:Name="button" Content="Play" HorizontalAlignment="Left" Margin="19,380,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Click="btinicio_Click" Height="44" Width="87" Background="#FFDDDDDD" FontWeight="Bold">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button x:Name="btstop" Content="Stop" HorizontalAlignment="Left" Margin="295,380,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Click="btstop_Click" Height="44" Width="87" FontWeight="Bold">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button x:Name="btstop_Copiar" Content="Pause" HorizontalAlignment="Left" Margin="111,380,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Click="btpause_Click" Height="44" Width="87" FontWeight="Bold">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button x:Name="btstop_Copiar1" Content="Resume" HorizontalAlignment="Left" Margin="203,380,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Click="btresume_Click" Height="44" Width="87" FontWeight="Bold">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>

    </Grid>
</Window>
